                <script>
                    // Get the modal
                    var programcihazimodal = document.getElementById("programcihazimodal");
                    var programtestmodal = document.getElementById("programtestmodal");
                    var programanalitmodal=document.getElementById("programanalitmodal");
                    // Get the button that opens the modal

                    var programcihazibutton = document.getElementById("programcihazibutton");
                    var programtestbutton = document.getElementById("programtestbutton");
                    var programanalitbutton=document.getElementById("programanalitbutton");
                    // Get the <span> element that closes the modal

                    var programcihazispan = document.getElementsByClassName("programcihaziclose")[0];
                    var programtestspan = document.getElementsByClassName("programtestclose")[0];
                    var programanalitspan=document.getElementsByClassName("programanalitclose")[0];
                    // When the user clicks the button, open the modal 

                    programcihazibutton.onclick = function () {
                        programcihazimodal.style.display = "block";
                    }
                    programtestbutton.onclick = function () {
                        programtestmodal.style.display = "block";
                    }

                    programanalitbutton.onclick = function () {
                        programanalitmodal.style.display = "block";
                    }


                    // When the user clicks on <span> (x), close the modal
                        programcihazispan.onclick = function () {
                            programcihazimodal.style.display = "none";
                    }

                    programtestspan.onclick = function () {
                        programtestmodal.style.display = "none";
                    }
                    programanalitspan.onclick = function () {
                        programanalitmodal.style.display = "none";
                    }



                    // When the user clicks anywhere outside of the modal, close it
                    window.onclick = function (event) {
                        if (event.target == programcihazimodal) {
                            programcihazimodal.style.display = "none";
                        }
                    }

                    window.onclick = function (event) {
                        if (event.target == programtestmodal) {
                            programtestmodal.style.display = "none";
                        }
                    }

                    window.onclick = function (event) {
                        if (event.target == programanalitmodal) {
                            programanalitmodal.style.display = "none";
                        }
                    }



                </script>




