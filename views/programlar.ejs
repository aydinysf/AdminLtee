                    programtestbutton.onclick = function () {
                        programtestmodal.style.display = "block";
                    }

                    programanalitbutton.onclick = function () {
                        programanalitmodal.style.display = "block";
                    }


                    // When the user clicks on <span> (x), close the modal
                    programcihazispan.onclick = function () {
                        programcihazimodal.style.display = "none";
                    }

                    programtestspan.onclick = function () {
                        programtestmodal.style.display = "none";
                    }
                    programanalitspan.onclick = function () {
                        programanalitmodal.style.display = "none";
                    }



                    // When the user clicks anywhere outside of the modal, close it
                    window.onclick = function (event) {
                        if (event.target == programcihazimodal) {
                            programcihazimodal.style.display = "none";
                        }
                    }

                    window.onclick = function (event) {
                        if (event.target == programtestmodal) {
                            programtestmodal.style.display = "none";
                        }
                    }

                    window.onclick = function (event) {
