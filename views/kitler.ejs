                <script>
                    // Get the modal
                    var kitkatalogmodal = document.getElementById("kitkatalogmodal");
                    var kitkatalogmodelmodal=document.getElementById("kitkatalogmodelmodal")
                    // Get the button that opens the modal
                    var kitkatalogbutton = document.getElementById("kitkatalogbutton");
                    var kitkatalogmodelbutton=document.getElementById("kitkatalogmodelbutton")
                    // Get the <span> element that closes the modal
                    var kitkatalogclose = document.getElementsByClassName("kitkatalogclose")[0];
                    var kitkatalogmodelclose = document.getElementsByClassName("kitkatalogmodelclose")[0];

                    // When the user clicks the button, open the modal 
                    kitkatalogbutton.onclick = function () {
                        kitkatalogmodal.style.display = "block";
                    }
                    kitkatalogmodelbutton.onclick = function () {
                        kitkatalogmodelmodal.style.display = "block";
                    }

                    // When the user clicks on <span> (x), close the modal
                        kitkatalogclose.onclick = function () {
                            kitkatalogmodal.style.display = "none";
                    }

                    kitkatalogmodelclose.onclick = function () {
                        kitkatalogmodelmodal.style.display = "none";
                    }

                    // When the user clicks anywhere outside of the modal, close it
                    window.onclick = function (event) {
                        if (event.target == kitkatalogmodal) {
                            kitkatalogmodal.style.display = "none";
                        }
                    }

                    window.onclick = function (event) {
                        if (event.target == kitkatalogmodelmodal) {
                            kitkatalogmodelmodal.style.display = "none";
                        }
                    }
